<!--pages/correct/correct.wxml-->
<map id="map"
 longitude="{{longitude}}" 
 latitude="{{latitude}}" 
 scale="14" 
 markers="{{markers}}" 
  polyline="{{polyline}}" 
  show-location 
  style="width: 100%; height: 300px;">
 <view class="orcanement">注意：请定位人员把标记点全部注释完以后 进行截屏操作保存在手机中!"</view>
  </map>

  <button bindtap="addPosition"  >添加标记点</button>
<view wx:for="{{markers}}" wx:key="property" wx:for-index="idx" wx:for-item="marker">
<view>序号{{marker.id+1}}，   经度：{{marker.latitude}}纬度：{{marker.longitude}} <input class="allclass " placeholder="注释地点" value="{{inputvalue}}"></input>

</view>
 </view>
